{
   "name": "axis-frontend",
   "private": true,
   "version": "0.0.0",
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "tsc -b && vite build",
      "lint": "eslint .",
      "preview": "vite preview",
      "type-check": "tsc --noEmit",
      "test:e2e": "playwright test",
      "test:e2e:ui": "playwright test --ui",
      "test:e2e:headed": "playwright test --headed",
      "test:trace": "playwright show-trace test-results/**/trace.zip",
      "clean": "rm -rf dist",
      "clean:tests": "rm -rf test-results playwright-report",
      "clean:all": "rm -rf dist build coverage .playwright node_modules/ test-results playwright-report",
      "clean:deps": "rm -rf node_modules package-lock.json bun.lock"
   },
   "dependencies": {
      "react": "^19.1.1",
      "react-dom": "^19.1.1",
      "react-icons": "^5.5.0",
      "react-router-dom": "^7.9.1",
      "styled-components": "^6.1.19"
   },
   "devDependencies": {
      "@eslint/js": "^9.35.0",
      "@playwright/test": "^1.55.0",
      "@swc/plugin-styled-components": "^9.1.0",
      "@types/node": "^24.5.2",
      "@types/react": "^19.1.13",
      "@types/react-dom": "^19.1.9",
      "@vitejs/plugin-react-swc": "^4.0.1",
      "eslint": "^9.35.0",
      "eslint-plugin-react-hooks": "^5.2.0",
      "eslint-plugin-react-refresh": "^0.4.20",
      "globals": "^16.4.0",
      "typescript": "~5.8.3",
      "typescript-eslint": "^8.43.0",
      "vite": "^7.1.6"
   }
}
